   field.setValues(protocol);

   if (protocol != httpsVal) {
      if (form.getValue("enforceHttps") == "true") {
         form.showFatal(form.getValue("httpsRequiredMessage"));
         form.disable("reason");
         var approveButton = document.getElementById('SubmitAction1'); 
         if (approveButton) {
