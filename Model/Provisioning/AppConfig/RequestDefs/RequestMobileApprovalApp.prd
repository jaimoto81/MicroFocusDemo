      <comment>function appendValue(url, urlParameter) {
   var val = flowdata.get("mobileConfig." + urlParameter);
   if (val &amp;&amp; (val.length &gt; 0)) {
      if (url.length == 0) {
         if (flowdata.get('Start/request_form/enterpriseApp') == "true") {
            url += "idm-ent-approval://settings/?";
          } else {
            url += "idmapproval://settings/?";
          }
      } else {
         url += "&amp;";
      }
      // special case for host info
      if ((urlParameter == "host") &amp;&amp; (flowdata.get('Start/request_form/protocol') == "http:")) {
         url += urlParameter + "=" + "http://" + val;
      } else {
         url += urlParameter + "=" + val;
      }
   }
   
  return url;
}

function getUserContainer(recipientDn) {
   var parts = recipient.split(",");
   var built = "";
   for (var i = 1; i &lt; parts.length; i++) {
      built += parts[i] + ",";
   }

  return encodeURIComponent(built.substring(0, built.length -1));
}

function buildUrl() {
try {
   var url = "";

   // host info
   url = appendValue(url, 'host');
   url = appendValue(url, 'port');
   url = appendValue(url, 'rbpmContext');
   url = appendValue(url, 'timeout');

   // user info
   url += "&amp;userid=";
   url += recipient.split(",")[0].split("=")[1];
   url += "&amp;userContainer=";
   url += getUserContainer(recipient);
   url += "&amp;namingAttribute=" + recipient.split(",")[0].split("=")[0];

   // this is only necessary if our api level is less than 4.1
   if ((parseInt(flowdata.get('version.major')) &lt; 4) || ((parseInt(flowdata.get('version.major')) == 4) &amp;&amp; (parseInt(flowdata.get('version.minor')) &lt;= 0 ))) { 
      url += "&amp;provAdminGetTasksWorkaroundInPlace=";
      url += RoleVault.isUserProvAdmin(recipient);
   }

   // vdx info
   url = appendValue(url, 'vdxUserEntity');
   url = appendValue(url, 'vdxFirstNameAttribute');
   url = appendValue(url, 'vdxLastNameAttribute');
   url = appendValue(url, 'vdxNameFormatAttribute');
   url = appendValue(url, 'vdxPhoneAttribute');
   url = appendValue(url, 'vdxMobileAttribute');
   url = appendValue(url, 'vdxPhotoAttribute');
   url = appendValue(url, 'vdxPhotoAttributeLdap');
   url = appendValue(url, 'vdxEmailAttribute');


   return "mail sent to " + flowdata.get('Start/request_form/email') + ": " + url;
} catch (ex) {
   return ex.toString();
}
}; buildUrl();</comment>
