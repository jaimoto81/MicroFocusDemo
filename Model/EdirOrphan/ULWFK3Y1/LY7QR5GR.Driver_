uot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&#x9;&lt;filter-attr attr-name=&quot;Password Minimum Length&quot;>&#xA;&#x9;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&#x9;&lt;filter-attr attr-name=&quot;Surname&quot;>&#xA;&#x9;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&#x9;&lt;filter-attr attr-name=&quot;Telephone Number&quot;>&#xA;&#x9;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&#x9;&lt;filter-attr attr-name=&quot;manager&quot;>&#xA;&#x9;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&#x9;&lt;filter-attr attr-name=&quot;Group Membership&quot;>&#xA;&#x9;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&lt;/filter-class>&#xA;&#x9;&lt;filter-class class-name=&quot;Group&quot;>&#xA;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;filter-attr attr-name=&quot;Member&quot;>&#xA;&#x9;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&#x9;&lt;filter-attr attr-name=&quot;Owner&quot;>&#xA;&#x9;&#x9;&#x9;&lt;package package-id=&quot;15NU2MJW_201109261950500421&quot; pkg-assoc-id=&quot;C3A99NI1_201109261954100906&quot;/>&#xA;&#x9;&#x9;&lt;/filter-attr>&#xA;&#x9;&lt;/filter-class>&#xA;&lt;/filter>"/>
